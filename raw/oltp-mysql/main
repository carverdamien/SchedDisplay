#!/bin/bash
set -u -e -x
: ${MONITOR}
! [ -f ${HDF5} ]
source cleanup
source prepare
[ -n "${MONITOR}" ] && source monitoring_start
/usr/bin/time \
    -f "%e" \
    ./run > >(tee output/time.out) 2> >(tee output/time.err) &
wait
[ -n "${MONITOR}" ] && source monitoring_stop
source report
source cleanup
